<configuration>
  <configSections>
    <section name="BotConfigSection" type="JuniperBot.Model.Configuration.BotConfigSection, JuniperBot" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <BotConfigSection SelfUpdateInterval="300000">
    <Discord Token="INSERT" CommandPrefix="!">
      <WebHooks>
        <!--<WebHook Id="INSERT" Token="INSERT" />-->
      </WebHooks>
    </Discord>
    <Instagram 
      ClientId="INSERT" 
      ClientSecret="INSERT" 
      Token="INSERT" 
      RedirectUri="INSERT" 
      UserId="2053300282" 
      TTL="300000" />
  </BotConfigSection>
  <log4net>
