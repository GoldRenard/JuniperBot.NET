      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d %t %-5p %c] %m%n" />
      </layout>
    </appender>
    <root>
      <level value="INFO" />
      <appender-ref ref="Console" />
    </root>
  </log4net>
